<Stories><List>

	<!-- 天玄大陆 -->
	<Story Name="Evil_Secrets_LiuYunCheng"  Parent="BaseFillingStory">
		<DisplayName>流云城</DisplayName>
		<Desc>[NAME]御剑飞行来到了流云城。</Desc>
		<Selections>
			<li>
				<Display>打探行踪</Display>
				<OKResult><![CDATA[
					EvilNpc:ShowInquireReincarnatesBox(me,world,{68061501,68061504});
				]]>
				</OKResult>
			</li>
			<li>
				<Display>寻觅功法</Display>
				<OKResult><![CDATA[
					if world:IsGongUnLocked("Evil_Gong_1") then
						me:AddMsg(XT("一无所获"));
					else
						me:AddMsg(XT("[NAME]在流云城萧门的一间别院中寻得一本功法。"));
						me:UnLockGong("Evil_Gong_1");
					end
				]]>
				</OKResult>
			</li>
			<li>
				<Display>离开此地</Display>
				<OKResult><![CDATA[
					me:AddMsg(XT("[NAME]不愿意冒险，离开了此地。"));
				]]>
				</OKResult>
			</li>
		</Selections>
	</Story>

